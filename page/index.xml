&lt;!-- ## Git 提交历史图

&lt;div id=&#34;git-graph&#34;&gt;&lt;/div&gt;

&lt;script&gt;
  fetch(`https://api.github.com/repos/${encodeURIComponent($repo)}/commits?sha=${encodeURIComponent($branch)}&amp;per_page=${encodeURIComponent($depth)}`)
    .then(response =&gt; response.json())
    .then(data =&gt; {
      const graphDiv = document.getElementById(&#39;git-graph&#39;);
      data.forEach(commit =&gt; {
        const commitDiv = document.createElement(&#39;div&#39;);
        commitDiv.textContent = `${commit.commit.author.name}: ${commit.commit.message}`;
        graphDiv.appendChild(commitDiv);
      });
    })
    .catch(error =&gt; console.error(&#39;Error fetching commits:&#39;, error));
&lt;/script&gt; --&gt;
